<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>
    <ul>
        <% list_song_id.forEach(function(song){ %>
            <div>
                <a href="/song?id=<%= song.encodeId %>">
                    <%= song.title %>
                </a>
            </div>
            <% }) %>
    </ul>
</body>

</html>